pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'Building World'
            }
        }
        stage('test') {
            steps {
                echo 'Testing World'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying World'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing World'
            }
        }
    }
}
